
public class FixedStackArray implements Stack {
	
	Object[] StackArray;
	int topOfStack = -1;
	
	public FixedStackArray(int StackSize) {
		StackArray = new Object[StackSize];
	}
	
	public void push(Object element) {
		StackArray[++topOfStack] = element;
	}
	
	public Object pop() {
		if(topOfStack==-1) {
			return null;
		}
		else {
			return StackArray[topOfStack--];
		}
		
		
	}
	
	public static void main(String[] args) {
		
		FixedStackArray CurrentStack = new FixedStackArray(5);
		Object a = "hello";
		Object b = "world";
		CurrentStack.push(a);
		CurrentStack.push(b);
		System.out.println(CurrentStack.pop());
		System.out.println(CurrentStack.pop());
	}

}
